{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python",
            "type": "python",
            "request": "launch",
            "stopOnEntry": false,
            "pythonPath": "${config:python.pythonPath}",
            "program": "${file}",
            "cwd": "${fileDirname}",
            "env": {
                "TF_CPP_MIN_LOG_LEVEL": 2
            },
            "envFile": "${workspaceRoot}/.env"
        }
    ]
}
